	<!-- This is the data set for RSS Feed Provider 1 -->
	<RssFeedSourceInfo>
            <rssFeedProviderName>gongjam's tumblr</rssFeedProviderName>
     	    <rssFeedProviderUrl>http://gongjam.tumblr.com/rss</rssFeedProviderUrl>
    	    <maximumRssItemsToBeReturned>5</maximumRssItemsToBeReturned>
 	</RssFeedSourceInfo>

	<!-- This is the data set for RSS Feed Provider 2 -->
	<RssFeedSourceInfo>
            <rssFeedProviderName>tutsplus ebooks</rssFeedProviderName>
     	    <rssFeedProviderUrl>http://ebooks.tutsplus.com/recently_added.atom</rssFeedProviderUrl>
